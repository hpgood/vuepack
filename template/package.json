{
  "name": "{{ name }}",
  "version": "0.1.0",
  "description": "{{ description }}",
  "main": "client/index.js",
  "scripts": {
    "test": "{{#testcafe}}testcafe all test/e2e/index.js --app \"npm run dev\" --app-init-delay 10000{{/testcafe}}",
    "build": "webpack --config build/webpack.prod.js",
    "dev": "node build/server.js"{{#electron}},
    "app": "cross-env NODE_ENV=development electron app/"{{/electron}}{{#eslint}},
    "lint": "xo **/client/**/*.{vue,js}{{#electron}} **/app/**/*.js{{/electron}} !node_modules/**"
    {{/eslint}}
  },
  "author": "{{ author }}",
  "license": "MIT",
  "xo": {
    "extends": "./.eslintrc",
    "esnext": true,
    "envs": [
      "browser"
    ]
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ],
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "component",
        [ {{#elementui}}
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          },{{/elementui}}
          {
            "libraryName": "mint-ui",
            "style": true
          }
        ]
      ]
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {}
    }
  },
  "browserslist": ["last 2 versions", "ie > 8"],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "babel-runtime": "^6.18.0",
    "js-md5": "^0.7.3",
    "mint-ui": "^2.2.13",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.2.0",
    "vue-router": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue-app": "^1.1.1",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "cross-env": "^2.0.0",
    {{#electron}}
    "electron-devtools-installer": "^2.0.1",
    "electron": "^1.4.2",
    {{/electron}}
    {{#eslint}}
    "xo": "^0.18.0",
    "eslint-config-vue": "latest",
    "eslint-plugin-vue": "latest",
    {{#testcafe}}
    "eslint-plugin-testcafe": "^0.2.0",
    {{/testcafe}}
    {{/eslint}}
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "raw-loader": "^0.5.1",
    "offline-plugin": "^4.5.3",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    {{#testcafe}}
    "testcafe": "^0.12.1",
    {{/testcafe}}
    "vue-loader": "^11.0.0",
    "vue-template-compiler": "^2.2.0",
    "webpack": "2.2.0",
    "webpack-dev-server": "^2.6.1"
  }
}
